package thread.threadPool;

public class FixPoolDemo {
	private static Runnable getThread(final int i) {
		return new Runnable() {
			@Override
			public void run() {
				try {
					Thread.sleep(500);
				} catch (InterruptedException e) {
					e.printStackTrace();
				}
				System.out.println(i);
			}
		};
	}

}
