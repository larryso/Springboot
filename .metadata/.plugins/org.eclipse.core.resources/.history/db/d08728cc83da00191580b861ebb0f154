package String;

import java.util.StringJoiner;

public class StringJoinerTest {
	public static void main(String[] args) {
		String[] ss = { "larry", "jenny", "Hellen" };
		StringJoiner join = new StringJoiner("$");
		for(String s: ss) {
			join.add(s);
		}
		System.out.println(join.toString());
	}

}
